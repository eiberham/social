{
  "name": "social",
  "version": "1.0.0",
  "description": "A social event scheduling and promoting app",
  "main": "index.js",
  "scripts": {
    "dev": "npm run build && npm run copy-files && nodemon build/src/server.js --ignore './client/'",
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc",
    "copy-files": "copyfiles -f -V ./src/certs/*.pem  ./build/src/certs/"
  },
  "keywords": [
    "social",
    "party",
    "salsa",
    "bachata",
    "rumba"
  ],
  "author": "Abraham",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^12.12.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.20.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.18.2",
    "helmet": "^3.21.2",
    "jsonwebtoken": "^8.5.1",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.2"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^3.0.0",
    "@types/bluebird": "^3.5.28",
    "@types/compression": "^1.0.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/helmet": "0.0.45",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/validator": "^10.11.3",
    "copyfiles": "^2.1.1",
    "tslint": "^6.0.0-beta0",
    "typescript": "^3.6.4",
    "webpack": "^4.41.2"
  }
}
